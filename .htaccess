
RewriteBase /
RewriteEngine On

RewriteBase /

RewriteCond %{HTTP_HOST} ^canales\.pe [NC]

RewriteCond %{SERVER_PORT} 80

RewriteRule ^(.*)$ https://sandbox.canales.pe/$1 [R,L]

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}.html -f
RewriteRule ^(.*)$ $1.html

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}.php -f
RewriteRule ^(.*)$ $1.php


Options All - Indexes

ErrorDocument 404 /404.html